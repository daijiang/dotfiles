"*":
  "atom-material-ui":
    colors: {}
    fonts: {}
    tabs:
      compactTabs: true
    treeView:
      compactList: true
    ui:
      panelContrast: true
  "autocomplete-bibtex":
    resultTemplate: "\\citep{[key]}"
    scope: ".source.gfm, .text.tex.latex, .text.md"
  "autocomplete-plus":
    fileBlacklist: [
      ".*"
    ]
  core:
    automaticallyUpdate: false
    disabledPackages: [
      "language-gfm"
      "fonts"
    ]
    packagesWithKeymapsDisabled: [
      "spell-check"
    ]
  editor:
    fontSize: 17
    invisibles: {}
    lineHeight: 1.6
    preferredLineLength: 90
    softWrap: true
    softWrapAtPreferredLineLength: true
  "exception-reporting":
    userId: "9c0e0784-7da7-a9d3-cd3b-80122c32ccb8"
  fonts: {}
  minimap:
    plugins:
      "find-and-replace": true
      "find-and-replaceDecorationsZIndex": 0
  "one-dark-ui":
    layoutMode: "Compact"
  "spell-check":
    grammars: [
      "text.tex.latex"
      "text.plain"
      "text.md"
    ]
  "vim-mode":
    startInInsertMode: true
  welcome:
    showOnStartup: false
  wordcount:
    alwaysOn: true
    ignorecode: true
    noextension: true
"source.gfm":
  editor:
    softWrap: true
"text.latex.tex":
  editor:
    softWrap: true
"text.md":
  editor:
    softWrap: true
